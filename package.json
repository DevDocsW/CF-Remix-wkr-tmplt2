{
  "name": "remix-cloudflare-worker-template",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "config/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md}\""
  },
  "devDependencies": {
    "prettier": "^2.6.2",
    "turbo": "^1.2.9",
    "typescript": "^4.6.4"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.5.5"
}
